; LOC_SECT_PROMPT
;
;   Prompts
;
[prompt]

1 = ZZZZZ, "セッション"


; LOC_SECT_MESSAGE
;
;   Informational and error messages
; 
[message]

1  = ZZZZZ, "メモリの割り当てに失敗しました。"

2  = ZZZZZ, "オブジェクトの読み込み中にエラーが発生しました。"

3  = ZZZZZ, "オブジェクトが見つかりません。"

4  = ZZZZZ, "''%1!' がありません。"

5  = ZZZZZ, "失敗"

6  = ZZZZZ, "ok"

7  = ZZZZZ, "インターフェースドライバに対するフェールオーバ。"

8  = ZZZZZ, "オープンしているセッションがありません。"

9  = ZZZZZ, "%1! を追加しました。"

10 = ZZZZZ, "%1! の追加中にエラーが発生しました。"

11 = ZZZZZ, "%1! を修正しました。"

12 = ZZZZZ, "%1! の修正中にエラーが発生しました。"

13 = ZZZZZ, "%1! を削除しました。"

14 = ZZZZZ, "%1! の削除中にエラーが発生しました。"

15 = ZZZZZ, "すべてのサーバオブジェクトを削除しました。"

16 = ZZZZZ, "サーバオブジェクトの削除中にエラーが発生しました。"

17 = ZZZZZ, "エラー：同一セッション内のオブジェクトにコピーしようとしました。"

18 = ZZZZZ, "セッション %2! の '%1!' をセッション %4! の '%3!' にコピーします。"

19 = ZZZZZ, "%1! のコピー中にエラーが発生しました。"

20 = ZZZZZ, "エラー：同一セッションにコピーしようとしました。"

21 = ZZZZZ, "セッション %1! にすべてをコピーします"

22 = ZZZZZ, "セッション %1! %2!"

23 = ZZZZZ, "current"

24 = ZZZZZ, "構文エラー"

25 = ZZZZZ, "余分な属性があります"


; LOC_SECT_ATTR
;
;   dscp attributes
;
[attr]

1  = ZZZZZ, "トランスポートタイプ"

2 =  ZZZZZ, "トランスポートアドレス"

3  = ZZZZZ, "サーバセキュリティ"

4  = ZZZZZ, "セキュリティメカニズム"

