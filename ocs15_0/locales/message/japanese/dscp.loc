7  = ZZZZZ, "識別名"

8  = ZZZZZ, "見つかったサーバオブジェクト"

9  = ZZZZZ, "バージョン"

10 = ZZZZZ, "サービス"

11 = ZZZZZ, "ステータス"

12 = ZZZZZ, "アドレス"

13 = ZZZZZ, "SYBASE"

14 = ZZZZZ, "設定"

15 = ZZZZZ, "DS 名"

16 = ZZZZZ, "HA フェールオーバサーバ"

; LOC_SECT_INPUT
;
;   User input prompts
;
[input]

1  = ZZZZZ, "すべてのサーバオブジェクトを削除してもよろしいですか? (Y/N)"


; LOC_SECT_COMMAND
;
;   dscp commands
;
[command]

1  = ZZZZZ, "help"

2  = ZZZZZ, "h"

3  = ZZZZZ, "?"

4  = ZZZZZ, "open"

5  = ZZZZZ, "close"

6  = ZZZZZ, "read"

7  = ZZZZZ, "add"

8  = ZZZZZ, "mod"

9  = ZZZZZ, "addattr"

10 = ZZZZZ, "del"

11 = ZZZZZ, "delete-all"

12 = ZZZZZ, "copy"

13 = ZZZZZ, "copyall"

14 = ZZZZZ, "to"

15 = ZZZZZ, "list"

16 = ZZZZZ, "all"

17 = ZZZZZ, "sess"

18 = ZZZZZ, "config"

19 = ZZZZZ, "exit"

20 = ZZZZZ, "quit"

21 = ZZZZZ, "switch"

22 = ZZZZZ, "#done"

23 = ZZZZZ, "#del"

24 = ZZZZZ, "Y"

25 = ZZZZZ, "y"

26 = ZZZZZ, "DSNAME"

27 = ZZZZZ, "SESS"

28 = ZZZZZ, "SERVER_NAME"

29 = ZZZZZ, "NAME1"

30 = ZZZZZ, "NAME2"


; LOC_SECT_HELP
;
;   dscp commands' functional descriptions
;
[help]

; help | h | ?
1  = ZZZZZ, "この画面。"

; open [DSNAME]
2  = ZZZZZ, "デフォルトのディレクトリサービスまたは DSNAME ディレクトリサービスのセッションをオープンします。"

; close [SESS]
3  = ZZZZZ, "SESS 番のセッション、または現在のセッションをクローズします。"

; sess
4  = ZZZZZ, "すべてのセッションをリストします。"

; [switch] SESS
5  = ZZZZZ, "SESS 番のセッションを現在のセッションにします。"

; add SERVER-NAME
6  = ZZZZZ, "ディレクトリサービスに新しいサーバオブジェクトを追加します。"

; read SERVER-NAME
7  = ZZZZZ, "サーバオブジェクトの内容を読み込みます。"

; mod SERVER-NAME
8  = ZZZZZ, "既存のサーバオブジェクトを修正します。"

; addattr SERVER-NAME
9  = ZZZZZ, "既存のサーバオブジェクトに属性を追加します。"

; del SERVER-NAME
10 = ZZZZZ, "既存のサーバオブジェクトを削除します。"

; list [all]
11 = ZZZZZ, "すべてのサーバオブジェクトをリストします。"

; copy NAME1 to {SESS NAME2 | SESS | NAME2}
12 = ZZZZZ, "オブジェクト NAME1 を、SESS 番のセッションまたは現在のセッションの新規オブジェクト NAME2 としてコピーします。"

; copyall to SESS
13 = ZZZZZ, "現在のセッションのすべてのサーバオブジェクトを SESS 番のセッションにコピーします。"

; delete-all
14 = ZZZZZ, "現在のセッションのすべてのサーバオブジェクトを削除します。"

; config
15 = ZZZZZ, "現在の設定をリストします。"

; exit | quit
16 = ZZZZZ, "プログラムを終了します。"

