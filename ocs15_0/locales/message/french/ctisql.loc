; LOC_SECT_PROMPTS
;
;   Prompt-related strings
;
[prompts]

1 = ZZZZZ, "Mot de passe : "

2 = ZZZZZ, "Confirmer  "

3 = ZZZZZ, "<enter> pour la page suivante, <a> pour afficher tout ou <q> pour quitter :  "

; LOC_SECT_INIT
;
;   initialization strings
;
[init]

1 = ZZZZZ, "Erreur fatale : échec d'allocation de mémoire."

2 = ZZZZZ, "Objet de localisation non reconnu. Utilisation de la valeur par défaut '%1!'."

3 = ZZZZZ, "Erreur interne : un type de localisation incorrect a été transmis à cs_locale()."

4 = ZZZZZ, "Tentative erronée de chargement d'une paire de tables de conversion."

5 = ZZZZZ, "Une erreur s'est produite lors de la tentative de configuration du contexte de l'application pour une utilisation avec des structures de localisation."

6 = ZZZZZ, "Le caractère de remplacement est trop long."

7 = ZZZZZ, "Le nom de l'application est trop long."

8 = ZZZZZ, "Longueur d'historique non valide."

; LOC_SECT_ERRORS
;
;   Error message strings
;
;   NOTE: Errors 1 through 7 are messages carried over from pre-ConnR11 versions
;   of isql.
;
[errors]

