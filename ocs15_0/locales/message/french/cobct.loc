[cobctruntime]
1 = ZZ000, "Une erreur irrécupérable s'est produite."
2 = ZA000, "Erreur interne."
3 = ZD000, "Réception imprévue de CS_COMPUTE_RESULT."
4 = ZE000, "Réception imprévue de CS_CURSOR_RESULT."
5 = ZF000, "Réception imprévue de CS_PARAM_RESULT."
6 = ZG000, "Réception imprévue de CS_ROW_RESULT."
7 = ZB000, "Aucun message renvoyé pour sqlca, SQLCODE ou SQLSTATE."
8 = ZC000, "La connexion n'a pas encore été définie."
9 = 00102, "Réception imprévue de CS_STATUS_RESULT."
10 = ZI000, "Réception imprévue de CS_DESCRIBE_RESULT."
11 = 22005, "Exception de données : erreur dans l'attribution du descripteur d'élément, type."
12 = JZ000, "Erreur d'allocation de mémoire."
13 = ZK000, "La version de SQL-Server doit être supérieure ou égale à 10."
14 = ZM000, "Erreur d'initialisation de Client Library."
15 = ZN000, "Erreur lors de la pose ou de la libération d'un mutex."
16 = 08002, "Nom de connexion utilisé."
17 = ZO000, "Une reprise haute disponibilité (HA) s'est produite."
