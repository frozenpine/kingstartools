6 = ZZZZZ, "Erreur d'allocation de mémoire."
7 = ZZZZZ, "Type non supporté."
8 = ZZZZZ, "La fonction CS-ALL-CAPS n'est pas supportée par Cobol."
9 = 22003, "Un nombre entier comporte trop de chiffres pour le PIC des données dont il est extrait."
10 = ZZZZZ, "Combination incorrecte d'arguments pour SYBSETSQLDA."
11 = ZZZZZ, "SQLDA-SQLN est inférieur à SQLDA-SQLD."
12 = ZZZZZ, "SQLDA-SQLD est négatif."

[cobctorigin]

1 = ZZZZZ, "Erreur interne"
2 = ZZZZZ, "Erreur d'appel de programme, probablement du code généré par le précompilateur"
